{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "quotemark": {
      "options": ["single", "jsx-double"],
      "severity": "error"
    },
    "no-implicit-dependencies": false,
    "no-submodule-imports": {
      "options": [
        "src",
        "assets"
      ],
      "severity": "error"
    }
  }
}
